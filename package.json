{
  "name": "qavibeapp",
  "version": "2.5.0",
  "description": "QAVibe - Free Enterprise Testing Tools - Central platform showcasing TestFlux dashboard and StackHealth scorecard with integrated blog and support system",
  "main": "public/index.html",
  "scripts": {
    "build": "node scripts/build.js",
    "dev": "npm run serve",
    "serve": "cd public && python3 -m http.server 8002",
    "serve-node": "npx http-server public -p 8002 -o",
    "build:prod": "NODE_ENV=production node scripts/build.js",
    "preview": "npm run build && npm run serve",
    "changelog": "node scripts/generate-changelog.js",
    "lighthouse": "lighthouse http://localhost:8002 --output html --output-path ./lighthouse-report.html",
    "validate": "html-validate public/index.html",
    "optimize": "npm run build:prod && npm run lighthouse",
    "blog:start": "node server/blog-server-db.js",
    "blog:dev": "nodemon server/blog-server-db.js",
    "api:start": "node server/api-server.js",
    "api:dev": "nodemon server/api-server.js",
    "start": "node server/blog-server-db.js",
    "start:all": "concurrently \"npm run api:start\" \"npm run blog:start\" \"npm run serve\"",
    "start:frontend": "npm run serve",
    "start:backend": "npm run blog:start",
    "start:api": "npm run api:start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "testing",
    "automation",
    "quality-assurance",
    "dashboard",
    "free-tools",
    "enterprise",
    "playwright",
    "k6",
    "zap",
    "ci-cd"
  ],
  "author": "QAVibe Team",
  "license": "MIT",
  "homepage": "https://qavibeapp.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apercallc/qavibeapp.git"
  },
  "bugs": {
    "url": "https://github.com/apercallc/qavibeapp/issues"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "html-validate": "^8.0.0",
    "http-server": "^14.1.1",
    "lighthouse": "^12.8.1",
    "nodemon": "^3.0.1"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^8.0.1",
    "marked": "^16.1.2",
    "multer": "^1.4.5-lts.1",
    "sqlite3": "^5.1.7"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "directories": {},
  "type": "commonjs"
}
